alias vm='cd /var/vagrant/Homestead && vagrant resume && vagrant ssh -- -R 9200:localhost:9200'
alias vup="vagrant up" #Bring vagrant up
alias vres="vagrant resume" #Resume a vagrant instance
alias vpov="vagrant provision" #Provision a server
alias vssh="vagrant ssh" #Ssh to the server
alias vsus="vagrant suspend" #Suspend vagrant server
alias vhalt="vagrant halt" #Stop vagrant
alias velastic="vagrant ssh -- -R 9200:localhost:9200" #Connect vagrant to external elastic search process
