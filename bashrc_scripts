alias w_internet="php /var/www/scripts/wait.php" #Tries to connect to internet for about an hour. If successfull passes to next process"

alias satis="php /var/www/satis/bin/satis" #Call the satis implimentation 

alias mvup="mv * .[^.]* .. ; cd .." #Move all files up
alias sizeof="du -ha" #Gets the size of a particular file or folder 

alias serve="python -m SimpleHTTPServer 8080" #Serves the current folder on port 8080

alias behat="vendor/bin/behat" #Run local installation of behat
alias elastic="/var/www/shared/elasticsearch-1.4.1/bin/elasticsearch"

